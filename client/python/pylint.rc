[MASTER]
ignore=schema,services

[BASIC]
good-names=i,j,k,ex,Run,_,u,v,q,r,g,x,y,z,w,setUpClass
variable-rgx=[a-z_][a-z0-9_]{1,30}$
method-rgx=[a-z_][a-z0-9_]{2,60}$

[DESIGN]
max-args=10
max-locals=20
max-attributes=10
max-returns=25
max-branches=25

[MESSAGES CONTROL]
disable=no-member,missing-docstring,similarities,locally-disabled,protected-access,too-few-public-methods,too-many-public-methods,fixme,no-name-in-module,unused-import,cyclic-import,import-error,consider-using-f-string,comparison-with-callable

[REPORTS]
reports=no
